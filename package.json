{
  "name": "@textactor/actors-explorer",
  "version": "0.2.0",
  "description": "A nodejs module for exploring TextActors(Named entities) in texts.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib",
    "clean-test-code": "rimraf ./lib/**/*.test.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textactor/actors-explorer.git"
  },
  "keywords": [
    "textactor",
    "actors-explorer"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textactor/actors-explorer/issues"
  },
  "homepage": "https://github.com/textactor/actors-explorer#readme",
  "devDependencies": {
    "@types/node": "latest",
    "@types/shortid": "^0.0.29",
    "ava": "^3.15.0",
    "rimraf": "^3.0.2",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@textactor/concept-domain": "^0.3.0",
    "@textactor/domain": "^0.3.0",
    "atonic": "^1.0.4",
    "@hapi/boom": "^9.1.2",
    "concepts-parser": "^1.5.4",
    "entity-finder": "^0.6.2",
    "partial-name": "^0.1.4",
    "same-names": "^0.2.0",
    "shortid": "^2.2.16",
    "standard-text": "^0.1.1",
    "text-country": "^0.1.2",
    "wiki-entity": "^0.4.4"
  }
}
